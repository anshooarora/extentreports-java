<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>

    <!-- report theme -->
    <!-- STANDARD, DARK -->
    <theme>DARK</theme>

    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>utf-16</encoding>

    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <!-- HTTP, HTTPS -->
    <protocol>HTTP</protocol>

    <!-- offline report -->
    <timelineEnabled>true</timelineEnabled>
    
    <!-- offline report -->
    <offlineMode>true</offlineMode>

    <!-- title of the document -->
    <documentTitle>Test1</documentTitle>

    <!-- report name - displayed at top-nav -->
    <reportName>Test2</reportName>

    <!-- timestamp format -->
    <timeStampFormat>MMM dd, yyyy HH:mm:ss a</timeStampFormat>

    <!-- custom javascript -->
    <js>js1</js>

    <!-- custom styles -->
    <css>css1</css>

  </configuration>
</extentreports>